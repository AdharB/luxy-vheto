{% if customer %}
               <script>
                  localStorage.setItem('customerEmail', '{{ customer.email }}')
               </script>
            {% endif %}
<style>
  #quiz-kit-app-wrapper {
    min-height: 700px;
    background-color: rgb(245, 243, 239);
  }
  
  [id^="quiz-kit-subscribe"] form input {
    margin: 0 auto;
  }
  
  #quiz-kit-app-spinner {
    display: flex;
    align-items: center;
    justify-content: center;
    width: 100%;
    height: 700px;
  }
  
  #quiz-kit-app-spinner svg {
    animation: rotating 2s linear infinite;
    width: 50px;
    height: 50px;
  }
  
  #quiz-kit-app-spinner svg circle {
    stroke-width: 5px;
    stroke-linecap: round;
    animation: dashing 1.5s ease-in-out infinite;
  }
  
  @keyframes rotating {
    100% {
      transform: rotate(360deg);
    }
  }

  @keyframes dashing {
    0% {
      stroke-dasharray: 1, 150;
      stroke-dashoffset: 0;
    }
    50% {
      stroke-dasharray: 90, 150;
      stroke-dashoffset: -35;
    }
    100% {
      stroke-dasharray: 90, 150;
      stroke-dashoffset: -124;
    }
  }
  
  @media screen and (max-width: 767px) {
	#quiz-kit-app-wrapper {
      min-height: auto;
    }
  }
</style>
<div id="quiz-kit-app-wrapper" data-quiz-kit-key="p4FhMvZVo">
  <div id="quiz-kit-app-spinner">
    <svg viewBox="0 0 50 50">
      <circle
        className="path"
        cx="25"
        cy="25"
        r="20"
        fill="none"
        strokeWidth="5"
        stroke="currentColor"
      />
    </svg>
  </div>
</div>