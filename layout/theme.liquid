      {{ 'https://fonts.googleapis.com/css?family=Open+Sans&display=swap' | stylesheet_tag }}
    {%- endif -%}

    {%- if template contains 'hair-color-classification' -%}
      {{ "https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js" | script_tag }}
    {%- endif -%}

    {{ content_for_header }}

    {%- include "favicon" -%}
    {%- include "liquid-json" -%}
