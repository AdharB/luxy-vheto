    {%- if template contains 'customers' -%}
      {{ "shopify_common.js" | shopify_asset_url | script_tag }}
      {{ "customer_area.js"  | shopify_asset_url | script_tag }}
    {%- endif -%}

    {%- include "minmaxify-head" -%}

    <script src="https://cdn-widgetsrepository.yotpo.com/v1/loader/4ovNBaW4otRyWlwJz803Jg" async></script>
</head>
  <body class="theme-{{ template.name }}">
    {%- if theme.name == "Develop" -%}
      {%- include "vendor-google-tag-manager-body" -%}
    {%- endif -%}

    {%- include "vendor-facebook-sdk" -%}
    {%- include "svg" -%}

    {%- unless theme_landing_page -%}
      <div class="position-relative">
        {%- section "shipping-notification" -%}
        {%- include "notification-bar" -%}
        {%- include "header" -%}
        {%- include "search-form" -%}
        {%- include "cart-reminder" -%}
      </div>
    {%- endunless -%}

    <main>
      <div id="{{ template.name }}-container">
        {{ content_for_layout }}
      </div>
    </main>

    {%- unless theme_landing_page -%}
      {%- include "footer" -%}
    {%- endunless -%}

    {%- include "gdpr-notice" -%}
    {%- include "mobile-menu" -%}
    {%- include "modal-close" -%}
    {%- include "cart-drawer" -%}

    {%- include "vendor-maxmind" -%}

    {{ "theme.min.js" | asset_url | script_tag }}

    {%- unless template.name == "blog" or template.name == "article" -%}
      {%- include "vendor-lucky-orange" -%}
    {%- endunless -%}

    {%- include "vendor-klaviyo" -%}
    {%- include "vendor-klaviyo-back-in-stock" -%}
    {%- include "vendor-yotpo" -%}
    {%- include "vendor-attentive" -%}
    {%- include "vendor-twitter" -%}
    {%- include "schema-article" -%}
    {%- include "schema-breadcrumbs" -%}
    {%- include "schema-organization" -%}
    {%- include "schema-product" -%}
    {%- include "schema-website" -%}

    {%- if theme.role == "main" -%}
      {%- include "google-remarketing" -%}
      {%- include "vendor-bing" -%}
      {%- include "vendor-criteo" -%}
      {%- include "vendor-pinterest" -%}
      {%- include "vendor-snapchat" -%}
    {%- endif -%}

    {%- if template == "page.careers" -%}
      {%- include "vendor-workable" -%}
    {%- endif -%}

    {%- include "vendor-helpdocs" -%}

