{%- assign url = nil -%}

{%- assign id = nil -%}

{%- for attribute in attribute_titles -%}
  {%- if attribute == "url" -%}
    {%- assign url = attribute_values[forloop.index0] -%}
  {%- endif -%}
{%- endfor -%}

{%- assign id = url | split: "/" | last -%}

{%- if url -%}
  <blockquote class="tiktok-embed" cite="{{ url }}" data-video-id="{{ id }}" style="max-width: 605px; min-width: 325px;">
    <section></section>
  </blockquote>

  <script async src="https://www.tiktok.com/embed.js"></script>
{%- endif -%}
