{% assign heading = section.settings.heading %}

{% assign description = section.settings.description %}

<div class="careers-section box-secondary-light">
  <div class="careers-section-container">
    <div class="career-perks-content">
      <h2 class="careers-section-title with-subtitle">
        {{ heading }}
      </h2>

      <p class="careers-section-description">
        {{ description }}
      </p>
    </div>

    <div class="grid">
      {% for block in section.blocks %}
        {% assign icon = block.settings.icon %}

        {% assign heading = block.settings.title %}

        {% assign description = block.settings.description | newline_to_br %}

        <div class="grid-item medium--one-half xlarge--one-quarter">
          <div class="careers-perks-thumbnail">
            <div class="local--icon">
              {{ icon }}
            </div>

            <h3 class="local--heading">
              {{ heading }}
            </h3>

            <p class="local--description">
              {{ description }}
            </p>
          </div>
        </div>
      {% endfor %}
    </div>
  </div>
</div>

